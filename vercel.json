{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["package.json", "package-lock.json", "node_modules/**"]
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "src/server.ts"
        }
    ],
    "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}